{
  "name": "Teltonika Server",
  "version": "0.2.5",
  "slug": "teltonika_server",
  "description": "Server for receiving data from Teltonika GPS devices using AVL protocol and logging to file with web interface",
  "startup": "services",
  "arch": ["amd64", "i386", "aarch64", "armv7", "armhf"],
  "boot": "auto",
  "panel_icon": "mdi:server-network-outline",
  "url": "https://github.com/PavelVe/home-assistant-addons/blob/main/TeltonikaServer/README.md",
  "homeassistant": "2022.1.1",
  "options": {
    "tcp_port": 3030,
    "web_port": 3031
  },
  "schema": {
    "tcp_port": "int",
    "web_port": "int"
  },
  "ingress": true,
  "host_network": true
}
